<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>startservlet</servlet-name>
  	<display-name>startservlet</display-name>
  	<description></description>
  	<servlet-class>startservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>inputserv</servlet-name>
  	<display-name>inputserv</display-name>
  	<description></description>
  	<servlet-class>input.inputserv</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>serv</servlet-name>
  	<display-name>serv</display-name>
  	<description></description>
  	<servlet-class>input.serv</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Serv</servlet-name>
  	<display-name>Serv</display-name>
  	<description></description>
  	<servlet-class>input.Serv</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadServlet</servlet-name>
  	<display-name>UploadServlet</display-name>
  	<description></description>
  	<servlet-class>UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>startservlet</servlet-name>
  	<url-pattern>/startservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>inputserv</servlet-name>
  	<url-pattern>/inputserv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>serv</servlet-name>
  	<url-pattern>/serv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Serv</servlet-name>
  	<url-pattern>/Serv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadServlet</servlet-name>
  	<url-pattern>/UploadServlet</url-pattern>
  </servlet-mapping>
</web-app>
